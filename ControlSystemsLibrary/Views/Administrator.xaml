<UserControl x:Class="ControlSystemsLibrary.Views.Administrator"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ControlSystemsLibrary.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="#FF5ECCFF">
        <StackPanel Width="200" VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBox Height="30"/>
            <Button Content="Lock" Height="35" Margin="0,5,0,0"/>
        </StackPanel>
    </Grid>
</UserControl>
